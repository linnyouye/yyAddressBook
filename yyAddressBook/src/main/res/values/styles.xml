<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="ListView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@null</item>
        <item name="android:layout_weight">1</item>
        <item name="android:divider">@null</item>
        <item name="android:cacheColorHint">@null</item>
        <item name="android:scrollbars">none</item>
        <item name="android:headerDividersEnabled">false</item>
        <item name="android:listSelector">@null</item>
    </style>

    <style name="GroupText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/group_bg</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">17dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textColor">@color/group_text</item>
    </style>

    <style name="BottomIbtn">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/child_tv_name</item>
    </style>

    <style name="BottomDivider">
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/list_divider</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>

    <style name="MyRadioButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/tab_text_selector</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="TitleBar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/title_bar_height</item>
        <item name="android:background">@drawable/title_bar_bg</item>
    </style>

    <style name="TitleButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">@dimen/title_btn_text</item>
        <item name="android:paddingRight">30dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:background">@null</item>
        <item name="android:textColor">@color/btn_back_text</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:gravity">center</item>
        <item name="android:drawableLeft">@drawable/ic_back</item>
    </style>

    <style name="TitleBarIbtn">
        <item name="android:layout_width">@dimen/title_ibtn_width</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">@drawable/ibtn_selector</item>
    </style>

    <style name="TvChildName">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">18dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/child_tv_name</item>
    </style>

    <style name="TvChildPhone">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">14dp</item>
        <item name="android:minWidth">120dp</item>
        <item name="android:textColor">@color/child_tv_phone</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="MyScrollView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:scrollbarStyle">outsideOverlay</item>
    </style>

    <style name="ScrollViewContainer">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:padding">10dp</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="SingleItem">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:background">@drawable/item_single_selector</item>
        <item name="android:clickable">true</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="ItemLeftText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/item_height</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:textSize">@dimen/item_text_size</item>
        <item name="android:textColor">@color/item_left_text</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="ItemRightText">
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/item_height</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:textSize">@dimen/item_text_size</item>
        <item name="android:textColor">@color/item_right_text</item>
    </style>

    <style name="BigButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:textSize">@dimen/btn_ok_text</item>
        <item name="android:textColor">@color/btn_ok_text</item>
        <item name="android:shadowColor">#000</item>
        <item name="android:shadowRadius">1</item>
        <item name="android:shadowDy">-1</item>
    </style>

    <style name="CustomDialog" parent="@android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="DialogButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">@drawable/ibtn_selector</item>
        <item name="android:textSize">@dimen/btn_ok_text</item>
        <item name="android:textColor">@color/child_tv_name</item>
    </style>

    <style name="ItemDivider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/item_line_dark</item>
    </style>

    <style name="ItemSpinner">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/item_height</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:background">@drawable/spinner_selector</item>
    </style>

    <style name="WholeItem">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:background">@drawable/item_whole</item>
        <item name="android:orientation">vertical</item>
        <item name="android:padding">1px</item>
    </style>

    <style name="FirstItem">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/item_first_selector</item>
        <item name="android:clickable">true</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MiddleItem">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/item_middle_selector</item>
        <item name="android:clickable">true</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="LastItem">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/item_last_selector</item>
        <item name="android:clickable">true</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="PopupWindowBtn">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/ibtn_selector</item>
        <item name="android:padding">10dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/child_tv_name</item>
    </style>

    <style name="DialogTitle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">#3377dd</item>
        <item name="android:padding">10dp</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">#fff</item>
        <item name="android:shadowColor">#000</item>
        <item name="android:shadowRadius">2</item>
    </style>

    <style name="DialogBtn">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/dialing_btn_selector</item>
        <item name="android:layout_margin">1px</item>
        <item name="android:textSize">22dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/btn_ok_text</item>
        <item name="android:shadowColor">#000</item>
        <item name="android:shadowRadius">1</item>
        <item name="android:shadowDy">-1</item>
    </style>

    <style name="ListItem">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/list_item_selector</item>
        <item name="android:padding">5dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:descendantFocusability">blocksDescendants</item>
    </style>

    <style name="RoundIbtn">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:src">@drawable/round_ibtn_selector</item>
    </style>

</resources>